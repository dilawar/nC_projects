<Lems xmlns="http://www.neuroml.org/lems/0.7.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.neuroml.org/lems/0.7.2  https://raw.githubusercontent.com/LEMS/LEMS/development/Schemas/LEMS/LEMS_v0.7.2.xsd">

    <DefaultRun component="sim1" reportFile="/public/dilawars/nC_projects/simple_cell/simulations/Sim_2/simulator.props" timesFile="/public/dilawars/nC_projects/simple_cell/simulations/Sim_2/time.dat"/>

        <!--Include standard NeuroML 2 ComponentType definitions-->

    <Include file="NeuroML2CoreTypes/Cells.xml"/>
    <Include file="NeuroML2CoreTypes/Networks.xml"/>
    <Include file="NeuroML2CoreTypes/Simulation.xml"/>

    

        <!--Include the generated NeuroML 2 files-->


        <!--   Channel/synapse files-->
    <Include file="KConductance.channel.nml"/>
    <Include file="LeakConductance.channel.nml"/>
    <Include file="NaConductance.channel.nml"/>
        <!--   Cell files-->
    <Include file="SampleCell.cell.nml"/>
        <!--   Network file-->
    <Include file="simple_cell.net.nml"/>
    

        <!--Note: this could be: Simulation id="sim1" ... , but Component type="Simulation" ... 
        is used to allow validation of this file according to the LEMS schema specified above...-->
    <Component type="Simulation" id="sim1" length="100.0ms" step="0.02ms" target="network_simple_cell">
        
        <Display id="SampleGraph" title="simple_cell: Default Simulation Configuration, SampleCellGroup, VOLTAGE" timeScale="1ms">
            <Line id="v" quantity="SampleCellGroup[0]/v" scale="1 mV" color="#000000" save="/public/dilawars/nC_projects/simple_cell/simulations/Sim_2/SampleCellGroup_0.0.dat"/>
        </Display>
    </Component>


</Lems>